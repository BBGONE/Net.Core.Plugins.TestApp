{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "FileLoggingOptions": {
    "LogDirectory": "Logs",
    "FileName": "log-",
    "FileSizeLimit": 4194304,
    "RetainedFileCountLimit": 10,
    "BatchSize": 64,
    "FlushPeriod": "00:00:02",
    "BackgroundQueueSize": 10000
  }
}
